{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example2.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "restaurantName": "niefojw38902u0309ji",
            "itemId": "noewcnwn2",
            "amount": 5
        }
    ],
    "required": [
        "restaurantName",
        "itemId",
        "amount"
    ],
    "properties": {
        "restaurantName": {
            "$id": "#/properties/restaurantName",
            "type": "string",
            "title": "The restaurantName schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "niefojw38902u0309ji"
            ]
        },
        "itemId": {
            "$id": "#/properties/itemId",
            "type": "string",
            "title": "The itemId schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "noewcnwn2"
            ]
        },
        "amount": {
            "$id": "#/properties/amount",
            "type": "integer",
            "title": "The amount schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                5
            ]
        }
    },
    "additionalProperties": true
}