{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example123.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "firstname": "Barack",
            "lastname": "Obama",
            "address": "Tutkijantie 2, 905790 Oulu",
            "phone": "0489852794",
            "email": "test@gmail.com",
            "password": "eiohwgn23801zrhuOefh9iWBEFIO"
        }
    ],
    "required": [
        "firstname",
        "lastname",
        "address",
        "phone",
        "email",
        "password"
    ],
    "properties": {
        "firstname": {
            "$id": "#/properties/firstname",
            "type": "string",
            "title": "The firstname schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "Barack"
            ]
        },
        "lastname": {
            "$id": "#/properties/lastname",
            "type": "string",
            "title": "The lastname schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "Obama"
            ]
        },
        "address": {
            "$id": "#/properties/address",
            "type": "string",
            "title": "The address schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "Tutkijantie 2, 905790 Oulu"
            ]
        },
        "phone": {
            "$id": "#/properties/phone",
            "type": "string",
            "title": "The phone schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "0489852794"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "test@gmail.com"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "isNotEmpty": true,
            "examples": [
                "eiohwgn23801zrhuOefh9iWBEFIO"
            ]
        }
    },
    "additionalProperties": false
}